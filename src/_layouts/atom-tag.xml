---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 <title>{{ pkg.short_title }} - {{ page.tag }}</title>
 <link href="{{ pkg.homepage }}{{ page.url }}" rel="self"/>
 <link href="{{ pkg.homepage }}/tag/{{ page.tag }}.html"/>
 <updated>{{ build.timestamp | attributeDate }}</updated>
 <id>{{ pkg.homepage }}/tag/{{ page.tag }}.html</id>
 <author>
   <name>{{ pkg.author.name }}</name>
   <email>{{ pkg.author.email }}</email>
 </author>
 {% for post in page.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="{{ pkg.homepage }}{{ post.url }}"/>
   <updated>{{ post.date | attributeDate }}</updated>
   <id>{{ pkg.homepage }}{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
</feed>
